{
  "compilerOptions": {
  
    "target": "es5",                    
    "module": "commonjs",  
    "allowJs":true,
    "outDir": "dist",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": false,                  
    "esModuleInterop": true,                 
    
    
    "skipLibCheck": true,                    
    "forceConsistentCasingInFileNames": true  
  }
}
